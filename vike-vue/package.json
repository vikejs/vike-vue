    },
    "./renderer/onRenderClient": {
      "default": "./dist/renderer/onRenderClient.js",
      "types": "./dist/renderer/onRenderClient.d.ts"
    },
    "./usePageContext": {
      "default": "./dist/components/usePageContext.js",
      "types": "./dist/components/usePageContext.d.ts"
    },
    "./useData": {
      "default": "./dist/components/useData.js",
      "types": "./dist/components/useData.d.ts"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "release": "release-me patch"
  },
  "peerDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "vike": "^0.4.152",
