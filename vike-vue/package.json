  },
  "devDependencies": {
    "@brillout/release-me": "^0.1.14",
    "@types/node": "^20.10.7",
    "@vitejs/plugin-vue": "^5.0.2",
    "typescript": "^5.3.3",
    "vike": "0.4.155",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vue": "^3.4.5",
    "vue-tsc": "^1.8.27"
